
{
  "name": "whatsapp-ai-bot-server",
  "version": "1.0.0",
  "description": "Servidor do bot WhatsApp com IA personalizada",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "clean": "rm -rf dist sessions",
    "clear-session": "npm run clean && echo 'Session cleared! Run npm start to reconnect.'",
    "logs": "tail -f logs/bot.log"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "@whiskeysockets/baileys": "^6.7.5",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.19.2",
    "form-data": "^4.0.0",
    "node-fetch": "^3.3.2",
    "pino": "^8.17.0",
    "qrcode-terminal": "^0.12.0",
    "sharp": "^0.33.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/qrcode-terminal": "^0.12.2",
    "pino-pretty": "^13.0.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
